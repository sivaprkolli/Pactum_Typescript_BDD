{
  "name": "PactumJS Demo",
  "version": "1.0.0",
  "description": "Automation Tests",
  "main": "cucumber.cjs",
  "scripts": {
  
    
  "start": "ts-node steps/test.ts",
  

  "start1": "tsc && node dist/steps/test.js",
  
    
  "test": "cucumber-js --profile smoke_default",
  
    
  "test1": "cucumber-js test",
  
    
  "report": "npx ts-node reporter/report.ts",
  
    
  "test:failed": "cucumber-js -p rerun @rerun.txt",
  
    
  "functional": "cucumber-js feature/*.features --import steps/*.ts"
  
  },
  
  "devDependencies": {
  "login-po": "./pages/login-po/",
  
    

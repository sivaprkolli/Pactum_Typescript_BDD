  "@types/node": "^20.12.7",
  
    
  "cucumber-tsflow": "^4.4.4",
  
    
  "mocha": "^10.4.0",
  
    
  "pactum": "^3.6.7",
  
    
  "tslib": "^2.6.2",
  
    
  "typescript": "^5.4.5"
  
  },
  
  "dependencies": {
  
    
  "@cucumber/cucumber": "^10.4.0",
  
    
  "npm": "^10.5.2",
  
    
  "ts-node": "^10.9.2"
  
  }
}
